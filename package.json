{
  "name": "go-better-auth",
  "type": "module",
  "version": "0.3.1",
  "description": "Official SDK for GoBetterAuth.",
  "license": "Apache-2.0",
  "homepage": "https://github.com/GoBetterAuth/go-better-auth-node-sdk#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GoBetterAuth/go-better-auth-node-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/GoBetterAuth/go-better-auth-node-sdk/issues"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./plugins": {
      "import": "./dist/plugins/index.js",
      "require": "./dist/plugins/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "cookie": "^1.1.1",
    "es-toolkit": "^1.44.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "bumpp": "^10.4.1",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
